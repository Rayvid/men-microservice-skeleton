{
  "server": {
    "envName": "development",
    "listenOnPort": 3000,
    "externalHostname": "localhost"
  },
  "logger": {
    "dir": "logs",
    "fileName": "combined.log",
    "level": {
      "__comment__": "Set to null to remove file transport alltogether, its not really needed when in microservice mode, set it to warn or other value if you want it anyway",
      "file": null,
      "console": "info"
    }
  },
  "db": {
    "mongo": {
      "fullConnString": "",
      "schema": "mongodb",
      "host": "mongo",
      "user": "",
      "password": "",
      "port": "",
      "database": "{dbName}",
      "params": "",
      "poolSize": 5
    }
  },
  "sentry": {
    "dsn": ""
  },
  "oauth2": {
    "_": "To login to remote server",
    "tokenHost": "https://auth.yourhost",
    "tokenPath": "/oauth/token",
    "audience": "https://github.com/Rayvid/men-microservice-skeleton",
    "__": "To verify incomnig requests",
    "jwksURI": "https://auth.yourhost/jwks"
  }
